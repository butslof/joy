@import '../global-variables'
#blog 
    padding-top 44px
    padding-bottom 255px !important
    .splide__pagination__page 
       background-color transparent !important
       border 1px solid branco !important
    .splide__pagination__page.is-active 
        background-color branco !important
        background linear-gradient(#ffffff 0%,#ffffff 100%) !important 
        border 1px solid #707070 !important
    .splide__pagination 
        bottom 26px
    .breadcrumb
        padding-top 0px !important
        padding-bottom 25px 
    .card-component-one
        // width 100% 
        width 95%
        margin 0 auto
        .img 
            
            &:before 
                width 405px 
            img 
                border-top-left-radius 15px 
                border-top-right-radius 15px 
                width 100% 
    .border-boxs 
        border-radius 15px
    .container-posts
        .container 
            padding-left 0px 
        .row 
            padding-left 0px 
    
    .slide-topo-blog 
        height: 450px;
        width 100%
        margin-bottom 36px
        img  
            height 450px
            width 100%
            object-fit cover 
            border-radius 15px
            filter brightness(0.6)  
           
        .bg 
            border-radius 15px
    .slick-list 
        border-radius: 6px;

    .card-component-one
        margin-bottom 39px 
        height 440px
        border-radius 15px
        .link 
            color secundaryColor 
            text-transform uppercase
            i 
                color secundaryColor 
        .date 
            color primaryColor  
            font-weight weight-strong 
            display block
            margin-bottom 6px

            i 
                color primaryColor
    .box-post 
        border-radius: 6px;
        img 
            width 100%
            height 198px
            object-fit cover 
            border-top-left-radius 15px
            border-top-right-radius 15px
            
        margin-bottom 37px
        .text 
            padding-top 27px
            padding-left 26px
            padding-right 26px
            padding-bottom 33px 

            h2 
                font-size 19px 
                font-weight bold
        .post-dates 
            margin-top 10px
            p 
                margin-right 4px
                color golden
                i 
                    color golden
    .box-sidebar
        margin-bottom 49px
        background-color branco 
        .btn-mais-category-blog 
            color primaryColor
            font-size 16px 
            font-weight 600 
            cursor pointer
        .logo-sidebar  
            width 100%
            display block
            padding-top 53px
            padding-bottom 11px
            margin-top -31px
            border-top-right-radius 15px
            border-top-left-radius 15px
            img
                margin-bottom 10px
        .text
            padding-bottom 20px
            padding-left 21px 
            padding-right 21px
        .bg 
            img 
                width 100%
                margin-bottom 18px
        .title-sidebar 
            font-size 33px 
            font-weight 600 
            color primaryColor
            line-height 1.19 
        ul 
            list-style-type none
            li
                &::before 
                    color primaryColor 
                    content "\2022"
                    margin-right 10px 
                    font-size 21px
                    vertical-align bottom

        a 
            color primaryTextColor
            font-size 14px
            font-weight bold 

        .box-post-visitados 
            margin-top: 15px;
            img 
                width: 62px;
                height: 62px;
                object-fit cover
                border-radius 15px
                &:before 
                    content ""
                    width 62px
                    height 62px
                    position absolute
                    background #2fb89e33
                    border-radius 15px
            
                
            .title 
                font-size 15px
                font-weight  900 
                margin-left: 12px;
                height: 62px;
                display: flex;
                align-items: center;
        .categories-sidebar 
            margin-top 15px
            padding-left 0

        .txt-redes
    
            strong
                font-size 14px

                span
                    font-size 14px
                    color primaryOrange
                    font-weight bold 
        .redes-footer 
            a 
                i 
                    font-size 20px
                    margin-right 5px
                    margin-left 10px 
                    color primaryColor
    padding-bottom 60px                    
    .side-news 
        text-align left !important
        padding-top 26px
        strong 
            font-size 22px
            font-weight 600
            line-height 24px
            span 
                font-weight bold
                color primaryOrange
                font-size 22px
        .btn-outline-secondary
            &:hover
                background primaryOrange
                i 
                    color branco 
        form 
            ::placeholder
                font-weight 500

    .slick-dots 
         
        display flex
        align-items center
        justify-content center
        margin-top -52px
        position relative
        li 
            list-style-type none
            button 
                font-size 0
                background goldenDark
                padding 5px
                border none
                border-radius 10px
                margin-right 6px
        .slick-active 
            button 
                background  primaryColor!important 
    .slide-topo-blog 
        .bg 
            &:before 
                content ""
                width 100%
                height 452px
                top 0px
                position absolute
                left 0
                border-radius 15px
                z-index 1
                background linear-gradient(rgba(18, 18, 18, 0) 0%, rgba(15, 15, 15, 0.53) 52.3%, #171717 100%)

                
        .txt-post-slide 
            position absolute
            margin-top -272px
            z-index 9
            margin-left 56px
            .title 
                color branco
                font-weight bold
                font-size 35px
                max-width 517px
                margin-bottom: 27px;
             
            p 
                color branco
                max-width: 538px;
    .nav-slider 
        display flex
        position absolute
        margin-top -256px
        width 100% 
        .prev 
            right 42px
            position absolute
            font-size 25px
            cursor pointer
            i
                color #ffffff9e
        .next 
            position absolute
            left 16px
            font-size 25px
            cursor pointer
            i
                color #ffffff9e
        
    #single-post 
        #section-download-catalogo
            margin-top 111px
            margin-bottom 53px
        .box-itens-servicos
            display flex
            gap 50px
            margin-bottom 10px
            .item 
                &:hover 
                    transition transform 1s
                    transform scale(1.1)
                img 
                    width 78px 
                    margin 0 
                    display initial 
                .title  
                    font-size 12px   
        
        .box-ideal 
            margin-top 50px
            .title-ideal 
                font-size 20px 
                color secundaryGrayTxts 
                font-weight bold  
            .title 
                font-size 16px  
            .item 
                &:hover 
                    transition transform 1s
                    transform scale(1.1)
                
            img 
                display initial !important 
            .splide__arrow 
                margin-top -30px
                background transparent
                svg
                    fill primaryColor
                    height 32.1px
                    width inherit
            .splide__arrow--prev 
                margin-left -18px
            .splide__arrow--next 
                margin-right -18px
        h1  
            font-size 40px
            font-weight 600
            color primaryColor
        .container-content 
            .date 
                margin-top 13px
                color primaryColor
                font-weight bold
                i 
                    color primaryColor
                    font-weight bold
            ul 
                margin-left: 23px;
            
            strong 
                color primaryTextColor
            h2 
                color  primaryColor
                font-size 20px
                font-weight 600

            h3 
                color  primaryColor
                font-size 20px
                font-weight 600
            h4 
                color  primaryColor
                font-size 20px
                font-weight 600
            h5 
                color  primaryColor
                font-size 20px
                font-weight 600
            h6 
                color primaryGrayColor
                font-size 20px
                font-weight 600 
            .marcadores 
                margin-top 42px
                .title 
                    font-size 20px 
                    font-weight bold 
                    color primaryColor
                    display block
                    margin-bottom 13px
                .box-marcadores 
                    display grid
                    grid-template-columns 1fr 1fr 1fr 1fr
                    gap 19px 
                    
                    .item 
                        border-radius 15px 
                        background-color #DCE3EB 
                        display flex 
                        align-items center 
                        justify-content center 
                        min-height 47px
                        &:hover 
                            transition transform 1s
                            transform scale(1.1)
                        .title-item
                            font-size 16px !important 
                            font-weight 400 
                            margin-bottom 0
                            text-align center
                            padding 5px

        .col-banner 
            img 
                height 334px
                width 100% 
                object-fit cover
        .container-content 
            img 
                max-width 100%
                display block 
                object-fit cover 
                border-radius 15px
            a 
                color primaryColor !important

            td,  th {
            border: 1px solid #ddd;
            padding: 8px;
            }

            tr:nth-child(even){background-color: #f2f2f2;}

            tr:hover {background-color: #ddd;}

            th {
            padding-top: 12px;
            padding-bottom: 12px;
            text-align: left;
            background-color: golden;
            color: white !important;
            }
            th  strong {
                color branco  !important
            } 
            table 
                margin-bottom: 23px;
        .title-artigos-rl 
            font-size 30px !important
            font-weight 600 
            color primaryColor !important
            margin-bottom 23px
            margin-top 63px
    .contato-none 
        display none 
    .box-sidebar-img  
        padding-top 30px   
        .text
            padding-top 40px
            margin-top 18px 
            background primaryColor 
            border-bottom-left-radius: 15px;
            border-bottom-right-radius: 15px;
            *
                color branco

    .box-institucional 
        background url('../../imgs/img-blog.jpg') 
        background-position center bottom 
        background-size cover
        height 460.15px 
        background-repeat no-repeat 
        .title-sidebar
            font-size 24px
            line-height 29px 
            margin-bottom 7px
            display block 
        .link 
            font-weight weight-strong 
            color primaryColor 
            text-decoration underline  
        p 
            margin-bottom 6px


    
      
    .redes-blog 
        .box 
            justify-content center 
            margin-left 21px
            i 
                color branco
                font-size 17px
                margin-right 14px
                width 27.32px
                text-align center
                height 27.32px
                padding 4px
                border-radius 3px 
                background  secundaryColor
    .txt-redes
        color branco !important 
        font-weight weight-strong 
        margin-right 10px

    .paginacao 
        margin-top 31px
        .page-numbers 
            border 1px solid #CACACA
            padding 10px 
            font-size 14px 
            color #032C55 
            font-weight weight-strong 
        .page-numbers.current 
            background-color secundaryColor 
            color branco 

    /* Small only */
    @media screen and (min-width: 320px) and (max-width: 63.9375em) 
        padding-bottom 74px !important
        .box-sidebar 
            border-radius 15px
            a 
                font-size 14px 
            .title-sidebar 
                font-size 18px 
            .btn-mais-category 
                margin-top 41px
                display block
                margin-bottom 12px
        .card-component-one 
            height auto 
            width 100%
            .img
                &:before
                    width 301px
        .container-posts
            .container 
                padding 0px 

        .box-post 
            text-align left
            .post-dates
                display  block !important

            .text 
                h2 
                    font-size 18px
        .mobile-p 
            padding 0px 
        .nav-slider 
            .prev 
                right: 16px
        .slide-topo-blog  
            height 400px 
            border-radius 15px
            img 
                // filter brightness(0.2) 
                height 100%
                border-radius 15px 
            .bg 
                height 400px 
                &:before 
                    height 100%
            .txt-post-slide 
                margin-top 140px
                z-index 9
                position relative
                margin-left 16px
                margin-right 10px
                text-align left
                .title  
                    margin-top -342px
                    max-width 320px
                    font-size 20px !important
                    text-align left
            .title-artigos-rl 
                font-size 21px 
        .slick-dots 
            // margin-top -33px
            margin-left -33px
        #single-post 
            text-align left 
            #section-download-catalogo 
                text-align center
            .box-itens-servicos 
                margin-bottom 44px 
                .splide__slide 
                    height 113.38px
                
                .splide__pagination
                    margin 0
                    bottom -19px
            .slide-ideal 
                .splide__pagination 
                    margin 0 
                    bottom -4px 
                .splide__slide 
                    height 145.25px
            
            .slick-dots 
                margin-left 0px
            h1 
                font-size 21px
            .container-content 
            
                h2 
                    font-size 17px
                h3 
                    font-size 17px
                h4 
                    font-size 17px
                h5 
                    font-size 17px
                h6 
                    font-size 17px
            .title-artigos-rl 
                font-size 21px !important 
                color primaryColor !important
                margin-top 34px 
            .relacionados
                margin-bottom 34px
            .slick-dots 
                margin-top -6px 
        .paginacao 
            margin-bottom 38px 
            margin-top 0px 
        .container-content 
            .marcadores 
                .box-marcadores
                    grid-template-columns 1fr 1fr !important
            



    /* Large and up */
    @media screen and (min-width: 64em)
        .sidebar 
            padding 0px
        #single-post 
            .container-content 
                padding-right 85px
        .box-post
            height 435px
        .page-padrao 
            padding-top 40px
            padding-bottom 60px
            .container-content 
                padding-left 85px 
            
        
            

       